    "import numpy as np\n",
    "\n",
    "#Configuraci칩n reproducible\n",
    "rng = np.random.default_rng(seed=42)\n",
    "\n",
    "condiciones = [\"Cond_A\", \"Cond_B\", \"Cond_C\"]\n",
    "sujetos = [f\"Sujeto_{i:02d}\" for i in range(1, 31)]  # Sujeto_01 ... Sujeto_30\n",
    "\n",
    "# Simulaci칩n de tiempos de reacci칩n (200800 ms)\n",
    "def simular_tiempos(n):\n",
    "    datos = rng.normal(loc=450, scale=90, size=n)\n",
    "    return np.clip(datos, 200, 800)\n",
    "\n",
    "datos = np.column_stack([\n",
    "    simular_tiempos(len(sujetos)),\n",
    "    simular_tiempos(len(sujetos)),\n",
    "    simular_tiempos(len(sujetos))\n",
    "]).round(2)\n",
    "\n",
    "#Promedios por condici칩n (con bucle) \n",
    "promedios_dict = {}\n",
    "for j, cond in enumerate(condiciones):\n",
    "    promedios_dict[cond] = round(datos[:, j].mean(), 2)\n",
    "\n",
    "#Mejor condici칩n por sujeto \n",
    "mejor_condiciones = []\n",
    "for fila in datos:\n",
    "    idx_min = np.argmin(fila)\n",
    "    mejor_condiciones.append(condiciones[idx_min])\n",
    "\n",
    "#Conteo de mejores condiciones \n",
    "conteo = {cond: 0 for cond in condiciones}\n",
    "for mc in mejor_condiciones:\n",
    "    conteo[mc] += 1\n",
    "\n",
    "#Impresiones\n",
    "print(\"=== Vista previa (primeros 5 sujetos) ===\")\n",
    "print(f\"{'Sujeto':<10} {'Cond_A':>10} {'Cond_B':>10} {'Cond_C':>10} {'Mejor_condici칩n':>18}\")\n",
    "print(\"-\"*62)\n",
    "for i in range(5):\n",
    "    print(f\"{sujetos[i]:<10} \"\n",
    "          f\"{datos[i,0]:>10.2f} {datos[i,1]:>10.2f} {datos[i,2]:>10.2f} \"\n",
    "          f\"{mejor_condiciones[i]:>18}\")\n",
    "print()\n",
    "\n",
    "print(\"=== Promedio por condici칩n ===\")\n",
    "for cond, val in promedios_dict.items():\n",
    "    print(f\"{cond:<10}: {val:.2f} ms\")\n",
    "print()\n",
    "\n",
    "print(\"=== Conteo de 'Mejor_condici칩n' ===\")\n",
    "for cond in condiciones:\n",
    "    print(f\"{cond:<10}: {conteo[cond]} sujetos\")\n",
    "print()\n",
    "\n",
    "print(\"=== Porcentaje de sujetos con mejor respuesta ===\")\n",
    "for cond in condiciones:\n",
    "    porcentaje = conteo[cond] / len(sujetos) * 100\n",
    "    print(f\"{cond:<10}: {porcentaje:.1f}%\")"
