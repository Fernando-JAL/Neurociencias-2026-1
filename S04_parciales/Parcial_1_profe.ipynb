    "import numpy as np\n",
    "\n",
    "#Generar la matriz\n",
    "rng = np.random.default_rng(seed=123)\n",
    "matriz = rng.integers(1, 11, size=(6, 6))\n",
    "\n",
    "#Calcular frecuencias con bucle \n",
    "frecuencias = {}\n",
    "for fila in matriz:\n",
