    "import numpy as np\n",
    "rng = np.random.default_rng(seed=7)\n",
    "n_muestras = 200\n",
    "n_areas = 4\n",
    "areas = [f\"츼rea_{i}\" for i in range(1, n_areas + 1)]\n",
    "\n",
    "# Simulaci칩n de series fMRI \n",
    "# Mezcla de componente com칰n + ruido para que haya correlaci칩n\n",
    "comun = rng.normal(0, 1, size=(n_muestras, 1))\n",
    "ruido = rng.normal(0, 1, size=(n_muestras, n_areas))\n",
    "series = 0.5 * comun + 0.5 * ruido\n",
    "\n",
    "# Calcular correlaciones de Pearson con bucle \n",
    "correlaciones = {}\n",
    "for i in range(n_areas):\n",
    "    for j in range(n_areas):\n",
    "        if i == j:\n",
    "            correlaciones[(areas[i], areas[j])] = 1.0\n",
    "        else:\n",
    "            x = series[:, i]\n",
    "            y = series[:, j]\n",
    "            r = np.corrcoef(x, y)[0, 1]\n",
    "            correlaciones[(areas[i], areas[j])] = r\n",
    "\n",
    "#Convertir a matriz \n",
    "matriz = np.zeros((n_areas, n_areas))\n",
    "for i in range(n_areas):\n",
    "    for j in range(n_areas):\n",
