    "import numpy as np\n",
    "rng = np.random.default_rng(seed=123)\n",
    "n_puntos = 10\n",
    "puntos = rng.uniform(0, 100, size=(n_puntos, 2))\n",
    "\n",
    "#Calcular distancias euclidianas con bucle \n",
    "distancias = {}\n",
    "for i in range(n_puntos):\n",
    "    for j in range(n_puntos):\n",
    "        dx = puntos[i, 0] - puntos[j, 0]\n",
    "        dy = puntos[i, 1] - puntos[j, 1]\n",
    "        d = np.sqrt(dx**2 + dy**2)\n",
    "        distancias[(i, j)] = d\n",
    "\n",
    "#Generar matriz\n",
    "matriz = np.zeros((n_puntos, n_puntos))\n",
    "for (i, j), d in distancias.items():\n",
    "    matriz[i, j] = d\n",
    "\n",
    "#Encontrar pares m치s cercano y m치s lejano (excluyendo diagonal)\n",
    "distancias_validas = {(i, j): d for (i, j), d in distancias.items() if i < j}\n",
    "par_cercano = min(distancias_validas, key=distancias_validas.get)\n",
    "par_lejano = max(distancias_validas, key=distancias_validas.get)\n",
    "\n",
    "#Impresiones bonitas \n",
    "print(\"=== Coordenadas de los puntos ===\")\n",
