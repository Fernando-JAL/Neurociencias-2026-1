    "print()\n",
    "\n",
    "print(\"=== Matriz de conectividad (correlaciones de Pearson) ===\")\n",
    "print(f\"{'':<10}\" + \"\".join([f\"{a:>12}\" for a in areas]))\n",
    "print(\"-\" * (12 * (n_areas + 1)))\n",
