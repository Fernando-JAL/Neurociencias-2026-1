    "        if num in frecuencias:\n",
    "            frecuencias[num] += 1\n",
    "        else:\n",
    "            frecuencias[num] = 1\n",
    "\n",
    "#Convertir a lista de tuplas y ordenar \n",
    "lista_ordenada = sorted(frecuencias.items(), key=lambda x: x[1], reverse=True)\n",
    "\n",
    "# Impresiones \n",
    "print(\"=== Matriz generada ===\")\n",
    "print(matriz)\n",
    "print()\n",
    "print(\"=== Diccionario de frecuencias ===\")\n",
    "print(frecuencias)\n",
    "print()\n",
    "print(\"=== Lista ordenada (n칰mero, frecuencia) ===\")\n",
